generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model user {
  id            String     @id @default(uuid())
  Name          String
  Age           Int
  Email_Address String  @unique
  Phone_Number  String
  Nationality   String
  Adhaar_Number String?
  Contact_Name  String
  Contact_Phone String
  Relationship  String
  password      String
  destination   String?
}
model location{
  id          String @id @default(uuid())
  uid         String
  location    String
  time        String
  safe        Boolean
}